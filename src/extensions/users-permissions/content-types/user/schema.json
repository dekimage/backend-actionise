      "type": "json"
    },
    "rewards_tower": {
      "type": "json"
    },
    "objectives_json": {
      "type": "json"
    },
    "objectives_counter": {
      "type": "json"
    },
    "streak_rewards": {
      "type": "json"
    },
    "friends_rewards": {
      "type": "json"
    },
