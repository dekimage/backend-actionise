    },
    "shared_buddies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "shared_by"
    },
    "expansions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::expansion.expansion"
