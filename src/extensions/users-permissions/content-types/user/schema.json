    "levelrewards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::levelreward.levelreward"
    },
    "streakrewards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::streakreward.streakreward"
