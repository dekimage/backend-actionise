{
  "kind": "collectionType",
  "collectionName": "problems",
  "info": {
    "singularName": "problem",
    "pluralName": "problems",
    "displayName": "Problem",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "other_names": {
      "type": "richtext"
    },
    "alternative": {
      "type": "richtext"
    },
    "source": {
      "type": "richtext"
    },
    "expected_time_type": {
      "type": "enumeration",
      "enum": [
        "minutes",
        "hours",
        "days",
        "months",
        "years"
      ],
      "default": "days"
    },
    "expected_time_amount": {
      "type": "integer",
      "default": 1
    },
    "realm": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::realm.realm",
      "inversedBy": "problems"
    },
    "cards": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::card.card",
      "inversedBy": "problems"
    }
  }
}
