{
  "kind": "collectionType",
  "collectionName": "faqs",
  "info": {
    "singularName": "faq",
    "pluralName": "faqs",
    "displayName": "Faq"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "question": {
      "type": "string"
    },
    "answer": {
      "type": "richtext"
    },
    "sortOrder": {
      "type": "integer",
      "default": 1
    },
    "isOpen": {
      "type": "boolean",
      "default": true
    },
    "card": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::card.card",
      "inversedBy": "faqs"
    }
  }
}
