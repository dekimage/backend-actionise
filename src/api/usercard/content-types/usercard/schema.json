{
  "kind": "collectionType",
  "collectionName": "usercards",
  "info": {
    "singularName": "usercard",
    "pluralName": "usercards",
    "displayName": "usercard",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "usercards"
    },
    "card": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::card.card"
    },
    "level": {
      "type": "integer",
      "default": 1
    },
    "quantity": {
      "default": 1,
      "type": "integer"
    },
    "completed": {
      "type": "integer",
      "default": 0
    },
    "completed_at": {
      "type": "biginteger"
    },
    "is_favorite": {
      "type": "boolean",
      "default": false
    },
    "is_new": {
      "type": "boolean",
      "default": false
    },
    "is_unlocked": {
      "type": "boolean",
      "default": false
    },
    "glory_points": {
      "type": "integer",
      "default": 0
    },
    "league": {
      "type": "enumeration",
      "enum": [
        "unranked",
        "bronze",
        "silver",
        "gold",
        "platinum",
        "diamond",
        "grandmaster"
      ],
      "default": "unranked"
    },
    "completed_progress_max": {
      "type": "integer",
      "default": 4
    },
    "completed_contents": {
      "type": "json"
    },
    "rating": {
      "type": "integer",
      "default": 0
    },
    "card_name": {
      "type": "string",
      "private": true,
      "default": "null"
    },
    "message": {
      "type": "text"
    },
    "isRated": {
      "type": "boolean"
    },
    "progressMap": {
      "type": "json"
    },
    "progressQuest": {
      "type": "json"
    },
    "user_name": {
      "type": "string"
    }
  }
}
